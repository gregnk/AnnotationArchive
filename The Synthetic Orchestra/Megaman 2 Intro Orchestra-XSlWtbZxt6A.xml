<annotation id="annotation_242898" type="text" style="popup" log_data="ei=Urs8XNHwJIXXjQSioZ2wAg&amp;a-type=1&amp;link=http%3A%2F%2Fwww.youtube.com%2Fwatch%3Fv%3DZXoGIwqqCyA&amp;a-v=XSlWtbZxt6A&amp;xble=1&amp;l-class=1&amp;a-id=annotation_242898" itct="CAUQs-UBGP___________wEiEwjR4cSK2-3fAhWFa4MKHaJQByYo-B0yAml2SKDvxrPb1tWUXQ">
